{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "client/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.js"
        },
        {
            "src": "/proxy",
            "dest": "api/index.js"
        },
        {
            "src": "/embed.js",
            "dest": "server/public/embed.js"
        },
        {
            "src": "/(.*)",
            "dest": "client/$1"
        }
    ]
}